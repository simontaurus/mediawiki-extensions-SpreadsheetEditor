{
    "manifest_version":2,
    "name":"SpreadsheetEditor",
    "version":"0.0.1",
    "author":[
        "[https://github.com/simontaurus Simon Stier]"
    ],
    "url":"https://github.com/simontaurus/mediawiki-extensions-SpreadsheetEditor",
    "descriptionmsg":"spreadsheet-editor-desc",
    "namemsg":"spreadsheet-editor-name",
    "license-name":"AGPL-3.0-or-later",
    "type":"editor",
    "requires":{
        "MediaWiki":">= 1.31"
    },
    "MessagesDirs":{
        "SpreadsheetEditor":[
            "i18n"
        ]
    },
    "ResourceModules":{
        "ext.SpreadsheetEditor":{
            "localBasePath":"modules/ext.SpreadsheetEditor",
            "remoteExtPath":"SpreadsheetEditor/modules/ext.SpreadsheetEditor",
            "styles":[
                "SpreadsheetEditor.css"
            ],
            "scripts":[
                "SpreadsheetEditor.js"
            ],
            "position":"bottom"
        }
    },
    "AutoloadClasses":{
        "SpreadsheetEditor":"includes/SpreadsheetEditor.php"
    },
    "Hooks":{
        "BeforePageDisplay":"SpreadsheetEditor::onBeforePageDisplay"
    }
}
